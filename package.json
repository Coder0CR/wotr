{
  "dependencies": {
    "@aws-sdk/client-s3": "^3.988.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "aws-cdk": "^2.1106.0",
    "aws-cdk-lib": "^2.238.0",
    "constructs": "^10.4.5"
  },
  "scripts": {
    "cdk": "cdk --app='npx ts-node infra/cdk/bin/wotr.ts'",
    "cdk:synth": "cd infra/cdk && cdk synth",
    "cdk:deploy": "cd infra/cdk && cdk deploy",
    "cdk:diff": "cd infra/cdk && cdk diff",
    "test": "cd infra/cdk && npm test",
    "test:integration": "cd infra/cdk && RUN_INTEGRATION_TESTS=true npm run test:integration",
    "test:lambda": "ts-node scripts/test-lambda.ts"
  }
}
